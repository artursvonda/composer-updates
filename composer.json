{
    "name": "artursvonda/composer-updates",
    "description": "Plugin for composer dependency updates and change-logs",
    "license": "MIT",
    "type": "composer-plugin",
    "authors": [
        {
            "name": "Arturs Vonda",
            "email": "arturs@artursvonda.lv"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "composer-plugin-api": "^1.0.0"
    },
    "require-dev": {
        "composer/composer": "1.*@dev"
    },
    "extra": {
        "class": "ComposerUpdates\\ComposerUpdatesPlugin",
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "ComposerUpdates\\": "src/ComposerUpdates/"
        }
    }
}
